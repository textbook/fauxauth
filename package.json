{
  "name": "fauxauth",
  "version": "1.2.1",
  "keywords": [
    "express",
    "mock",
    "oauth",
    "stub",
    "test",
    "testing"
  ],
  "bin": {
    "fauxauth": "./lib/server.js"
  },
  "files": [
    "/lib"
  ],
  "description": "Helper application for testing OAuth clients",
  "scripts": {
    "pree2e": "docker-compose build",
    "e2e": "docker-compose run e2e run -d e2e:test",
    "e2e:test": "jest --config jest.config.e2e.js",
    "poste2e": "docker-compose down",
    "prebuild": "rimraf lib/*",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "prepublishOnly": "npm run build",
    "ship": "npm run lint && npm run test && npm run build && npm run e2e",
    "start": "node lib/server.js",
    "test": "jest",
    "test:cover": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/fauxauth.git"
  },
  "author": "Jonathan Sharpe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/fauxauth/issues"
  },
  "homepage": "https://github.com/textbook/fauxauth#readme",
  "dependencies": {
    "express": "^4.16.4",
    "jiff": "^0.7.3",
    "morgan": "^1.9.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/morgan": "^1.7.35",
    "@types/request": "^2.48.1",
    "@types/supertest": "^2.0.7",
    "@types/xml2js": "^0.4.3",
    "jest": "^23.6.0",
    "request": "^2.88.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.4"
  }
}
