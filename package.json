{
  "name": "fauxauth",
  "version": "2.1.0",
  "keywords": [
    "express",
    "mock",
    "oauth",
    "stub",
    "test",
    "testing"
  ],
  "bin": {
    "fauxauth": "./lib/server.js"
  },
  "files": [
    "/lib"
  ],
  "description": "Helper application for testing OAuth clients",
  "scripts": {
    "pree2e": "docker-compose build",
    "e2e": "docker-compose run e2e run -d test || (npm run poste2e && exit 1)",
    "poste2e": "docker-compose down",
    "e2e:all": "for NODE in carbon dubnium \"$(cat .nvmrc)\"; do NODE_RELEASE=$NODE npm run e2e; done",
    "prebuild": "rimraf lib/*",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "ship": "npm run lint && npm run test && npm run build && npm run e2e:all",
    "start": "node $npm_package_bin_fauxauth",
    "test": "jest",
    "test:report": "npm run test -- --coverage --reporters=\"default\" --reporters=\"jest-junit\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/fauxauth.git"
  },
  "author": "Jonathan Sharpe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/fauxauth/issues"
  },
  "homepage": "https://github.com/textbook/fauxauth#readme",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jiff": "^0.7.3",
    "morgan": "^1.9.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.25",
    "@types/morgan": "^1.7.35",
    "@types/supertest": "^2.0.7",
    "@types/xml2js": "^0.4.4",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2"
  }
}
