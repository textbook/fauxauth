{
  "name": "fauxauth",
  "version": "6.0.2",
  "keywords": [
    "express",
    "mock",
    "oauth",
    "stub",
    "test",
    "testing"
  ],
  "bin": {
    "fauxauth": "./lib/server.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/views"
  ],
  "description": "Helper application for testing OAuth clients",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "e2e": "npm --prefix=e2e run e2e:dev",
    "prebuild": "rimraf lib/*",
    "build": "tsc",
    "install:all": "npm ci && npm --prefix=e2e ci && npm --prefix=e2e run chromedriver",
    "lint": "eslint --ext=js,ts .",
    "ship": "npm run lint && npm run test && npm run build && npm run e2e",
    "start": "node $npm_package_bin_fauxauth",
    "test": "jest",
    "test:report": "npm run test -- --coverage --reporters=\"default\" --reporters=\"jest-junit\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/fauxauth.git"
  },
  "author": "Jonathan Sharpe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/fauxauth/issues"
  },
  "homepage": "https://github.com/textbook/fauxauth#readme",
  "funding": "https://ko-fi.com/textbook",
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.17.3",
    "jiff": "^0.7.3",
    "lodash.clonedeep": "^4.5.0",
    "morgan": "^1.10.0",
    "pug": "^3.0.2",
    "xml2js": "^0.4.23",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@codeyourfuture/eslint-config-standard": "^3.0.2",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.5",
    "@types/jest": "^27.0.2",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/morgan": "^1.7.35",
    "@types/supertest": "^2.0.7",
    "@types/xml2js": "^0.4.4",
    "@types/yargs": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.0",
    "jest-junit": "^13.0.0",
    "rimraf": "^3.0.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
