{
  "name": "fauxauth",
  "version": "0.6.1",
  "keywords": [
    "express",
    "mock",
    "oauth",
    "stub",
    "test",
    "testing"
  ],
  "bin": {
    "fauxauth": "./lib/server.js"
  },
  "description": "Helper application for testing OAuth clients",
  "scripts": {
    "babel": "babel src --out-dir lib",
    "pree2e": "docker-compose build",
    "e2e": "docker-compose run e2e run -d e2e:test",
    "e2e:test": "jest --config jest.config.e2e.json",
    "poste2e": "docker-compose down",
    "prebuild": "rimraf lib/*",
    "build": "npm run babel",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "ship": "npm run lint && npm run test && npm run build && npm run e2e",
    "start": "node lib/server.js",
    "test": "jest",
    "test:cover": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/fauxauth.git"
  },
  "author": "Jonathan Sharpe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/fauxauth/issues"
  },
  "homepage": "https://github.com/textbook/fauxauth#readme",
  "dependencies": {
    "express": "^4.16.4",
    "jiff": "^0.7.3",
    "morgan": "^1.9.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.7.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.12.1",
    "request": "^2.88.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0"
  }
}
