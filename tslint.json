{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "arrow-parens": true,
    "interface-name": false,
    "quotemark": [true, "double", "avoid-escape"],
    "semicolon": [true, "always"],
    "trailing-comma": [true, { "multiline": "always", "singleline": "never" }]
  },
  "rulesDirectory": []
}
